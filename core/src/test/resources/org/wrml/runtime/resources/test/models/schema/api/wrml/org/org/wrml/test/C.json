            }
        },
        {
            "name": "cDouble",
            "value": {
                "schemaUri": "http://schema.api.wrml.org/org/wrml/model/schema/DoubleValue"
            }
        },
        {
            "name": "cFlag",
            "value": {
                "schemaUri": "http://schema.api.wrml.org/org/wrml/model/schema/BooleanValue",
                "default": true
            }
        }
    ]
